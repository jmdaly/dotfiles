{
  "nativeMCPServers": {
    "neovim": {
      "disabled": true
    },
    "mcphub": {
      "disabled": false
    }
  },
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "env": {
        "DEBUG": "true",
        "API_KEY": "",
        "SERVER_URL": null
      },
      "disabled": true,
      "command": "uvx"
    },
    "serena": {
      "disabled": true,
      "args": [
        "run",
        "--directory",
        "/home/host/git/serena",
        "serena-mcp-server"
      ],
      "command": "uv"
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "disabled": true
    },
    "desktop-commander": {
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ],
      "disabled": true,
      "command": "npx"
    },
    "atlassian": {
      "url": "https://mcp.atlassian.com/v1/sse",
      "disabled": true
    }
  }
}
