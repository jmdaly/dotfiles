#%Module1.0#####################################################################
##
## Ford Proxy modulefile
##
proc ModulesHelp { } {
	puts stderr "Lost Ford proxy"
}

module-whatis	"loads Ford proxy"

set PROXY http://internet.ford.com:83
set NO_PROXY "localhost,127.0.0.1,.ford.com,*.ford.com,10.1.0.3,192.168.1.26"

setenv http_proxy ${PROXY}
setenv HTTP_PROXY ${PROXY}

setenv https_proxy ${PROXY}
setenv HTTPS_PROXY ${PROXY}

setenv no_proxy ${NO_PROXY}
setenv NO_PROXY ${NO_PROXY}