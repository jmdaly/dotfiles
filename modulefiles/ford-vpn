#%Module1.0#####################################################################
##
## Ford VPN modulefile
##
proc ModulesHelp { } {
	puts stderr "Loads Ford proxy"
}

module-whatis	"loads Ford VPN"

set-alias ford-vpn-up   "nmcli c u 'Ford VPN' --ask"
set-alias ford-vpn-down "nmcli c d 'Ford VPN'"
