proc ModulesHelp { } {
        global version prefix

        puts stderr "\tQt - Load commercial Qt"
}

module-whatis	"loads Qt 5.12"

set QT_VERSION       5.12.7
set QT_SHORT_VERSION 5.12.7
prepend-path PATH /opt/Qt/${QT_VERSION}/${QT_SHORT_VERSION}/gcc_64/bin
prepend-path PATH /opt/Qt/Tools/QtCreator/bin

